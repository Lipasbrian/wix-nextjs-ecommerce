    "@upstash/redis": "^1.34.7",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.2",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "plaiceholder": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.55.0",
