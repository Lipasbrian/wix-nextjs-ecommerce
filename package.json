  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "Remove-Item -Recurse -Force .next/cache, .next/.cache, rm -rf .next node_modules, rimraf .next node_modules",
    "clean-all": "Remove-Item -Recurse -Force .next, node_modules, package-lock.json",
