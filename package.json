{
  "name": "ecomstarter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node -e \"if (!require('fs').existsSync('./public/uploads')) { require('fs').mkdirSync('./public/uploads', { recursive: true }); }\"",
    "prebuild": "node -e \"if (!require('fs').existsSync('./public/uploads')) { require('fs').mkdirSync('./public/uploads', { recursive: true }); }\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "Remove-Item -Recurse -Force .next/cache, .next/.cache, rm -rf .next node_modules, rimraf .next node_modules",
    "clean-all": "Remove-Item -Recurse -Force .next, node_modules, package-lock.json",
    "test:db": "ts-node -r tsconfig-paths/register -P tsconfig.test.json src/__tests__/dbtest.ts",
    "type-check": "tsc --noEmit",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "lint:fix": "next lint --fix"
  },
  "prisma": {
    "seed": "ts-node -P tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
