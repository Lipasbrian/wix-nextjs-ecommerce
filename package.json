    "eslint-config-next": "14.2.2",
    "postcss": "^8",
    "prisma": "^6.6.0",
    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
